<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>duplicati-server</Name>
  <Description></Description>
  <Registry>https://hub.docker.com/r/ccormier/duplicati-server</Registry>
  <Repository>ccormier/duplicati-server</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8200</HostPort>
        <ContainerPort>8200</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/duplicati-server/</HostDir>
      <ContainerDir>/root/.config/Duplicati</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/backups/</HostDir>
      <ContainerDir>/backups</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:8200]/</WebUI>
  <Banner></Banner>
  <Icon>http://www.duplicati.com/_/rsrc/1316866598425/config/customLogo.gif</Icon>
  <ExtraParams></ExtraParams>
</Container>
